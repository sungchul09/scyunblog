{
  "extends": [
    "next/core-web-vitals",
    "plugin:@pandacss/recommended",
    "plugin:jsx-a11y/recommended",
    "plugin:prettier/recommended",
    "prettier"
  ],
  "plugins": ["@pandacss", "jsx-a11y", "prettier"],
  "rules": {
    "no-console": "warn",
    "prettier/prettier": [
      "error",
      {
        "endOfLine": "auto"
      }
    ],
  }
}
